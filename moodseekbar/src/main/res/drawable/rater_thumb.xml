<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="40dp" />
            <size
                android:width="60dp"
                android:height="60dp" />
            <stroke
                android:width="1dp"
                android:color="#EAEEF0" />
            <solid android:color="#EAEEF0" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="40dp" />
            <size
                android:width="10dp"
                android:height="10dp" />
            <stroke
                android:width="2dp"
                android:color="#F0B60A" />
        </shape>
    </item>
</layer-list>
