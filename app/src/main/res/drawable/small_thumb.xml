<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <size
                android:width="30dp"
                android:height="30dp" />
            <stroke
                android:width="1dp"
                android:color="#EAEEF0" />
            <solid android:color="#EAEEF0" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="40dp" />
            <size
                android:width="10dp"
                android:height="10dp" />
            <stroke
                android:width="1dp"
                android:color="#F0B60A" />
        </shape>
    </item>
</layer-list>
